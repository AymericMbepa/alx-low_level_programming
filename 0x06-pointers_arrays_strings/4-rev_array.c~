#include "main.h"

/**
 * strcmp - compare two strings
 * @a: a pointer to an array
 * @n: an input integer
 */
void reverse_array(int *a , int n)
{
	int i = n - 1, j = 0, k, l = 0;

	for (; i >= 0; i--)
	{
		l = *(a + i);
		tab[j] = *(a + i);
		j++;
	}
	for (k = 0; k < n; k++)
		*(a + k) = tab[k];
}
